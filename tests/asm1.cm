int ack(int x, int y)
{
    int z;
    if(x == 0)
    {
        return y + 1;
    }
    if(y == 0)
    {
        return ack(x - 1, 1);
    }
    z = ack(x-1, ack(x,y-1));
    return z;

}


int test(int a, int b)
{
    if(a == 0)
    {
        return b + 1;
    }
    if(a == 1)
    {
        return b - 1;
    }
    return b + 5;

}

void main(void)
{
    output(test(1, 10));
}
