/* Test 4 - no errors - more complex and while loops */


int mul(int a, int b)
{
    return a * b;
}


int exponent(int base, int power)
{
    int i;
    int c;
    i = power;
    c = base;
    while(i > 0)
    {
        c = mul(c, base);
        i = i - 1;
    }
    return c;
}





void main(void)
{
    int base; 
    int power; 
    base = 2;
    power = 8;
    
    output(exponent(base, power));

}
